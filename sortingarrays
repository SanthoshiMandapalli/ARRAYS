#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int a[5]={234,76,56,890,0};
    sort(a,a+5);
    for(int i=0;i<5;i++)
    {
	cout<<a[i]<<"\n";
    }
    cout<<"\n";
     sort(a,a+5,greater<int>());
     for(int i=0;i<5;i++)
    {
	cout<<a[i]<<"\n";
    }
	return 0;
}
